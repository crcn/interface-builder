{
  "name": "paperclip",
  "version": "11.0.3",
  "description": "- Realtime editing. Biggest priority is to have a bundler that  - Interoperability. Paperclip should work with any web framework.  - Type safety.  - Visually editable.",
  "main": "index.js",
  "scripts": {
    "test": "mocha ./lib/test",
    "install": "npm run build:node",
    "build": "npm run build:node && npm run build:native",
    "build:watch": "tsc --watch & (cd native && cargo watch -x 'build --release')",
    "build:node": "tsc",
    "build:native": "cd native && cargo build --release",
    "build:neon": "neon build --release",
    "build:electron": "electron-build-env neon build --release",
    "build:wasm:node": "wasm-pack build --target nodejs"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "get-port": "^5.1.1",
    "jayson": "^3.2.0",
    "mocha": "^7.0.1",
    "neon": "^2.0.0",
    "typescript": "^3.7.5"
  },
  "gitHead": "02c551ba2a2d5ec9ee9ee4e59b0a9e918646d248",
  "devDependencies": {
    "electron": "^6.1.7",
    "electron-build-env": "^0.2.0",
    "neon-cli": "^0.3.3"
  }
}
